
@{
    ViewBag.Title = "Departments";
}
@model List<StudentMS.Models.Department>
<h2>Departments</h2>
<div class="container">
    <table class="table table-condensed">
        <tr>
            <th>Id</th>
            <th>Name</th>
        </tr>
        @foreach (var dept in Model)
        {
            <tr>
                <td>@dept.Id</td>
                <td>@dept.Name</td>
            </tr>
        }
    </table>
</div>


